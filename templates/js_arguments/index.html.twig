{% extends 'base.html.twig' %}

{% block title %}JS - function arguments{% endblock %}

{% block stylesheets %}
    <style>
        body{
            background-color: #eee;
        }
        h3{
            background-color: #fff;
            padding: 10px;
            border: 1px solid #aaa;
            border-radius: 5px;
        }
    </style>
{% endblock %}

{% block body %}
    <div>
        <h3>
            <code>dev-tools -> console -> a(%arguments%);</code>
        </h3>
    </div>
{% endblock %}

{% block javascripts %}
    <script>
        function a(){
            arr = [];
            for(i=0;i<arguments.length;i++){
                console.log(arguments[i]);
                arr[i] = arguments[i];
            }

            return arr;
        }
    </script>
{% endblock %}